{
  "name": "review-me",
  "version": "0.0.1",
  "description": "Manage your github code review requests",
  "main": "./main.js",
  "scripts": {
    "test": "test",
    "start": "npm run build && electron .",
    "build": "rollup -c",
    "dist": "electron-builder ",
    "demo": "electron-builder --dir"
  },
  "build": {
    "appId": "com.review-me",
    "asar": false,
    "icon": "assets/icon.png",
    "mac": {
      "category": "productivity"
    },
    "extraResources": [
      {
        "from": "./build",
        "to": "./app/build",
        "filter": [
          "*"
        ]
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frodrigues-kununu/review-me.git"
  },
  "keywords": [
    "polymer",
    "electron",
    "PR"
  ],
  "author": "FÃ¡bio Rodrigues, Jacinto Rodrigues",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/frodrigues-kununu/review-me/issues"
  },
  "homepage": "https://github.com/frodrigues-kununu/review-me#readme",
  "devDependencies": {
    "@webcomponents/webcomponentsjs": "^2.2.10",
    "electron": "^5.0.1",
    "electron-builder": "^20.39.0",
    "rollup": "^1.11.3",
    "rollup-plugin-node-resolve": "^4.2.3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "lit-element": "^2.1.0"
  }
}